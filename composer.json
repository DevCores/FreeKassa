{
    "name": "maksa988/laravel-freekassa",
    "description": "FreeKassa payments for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "maksa988",
            "email": "maksa988ua@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "^5.3.10",
        "guzzlehttp/guzzle": "~6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Maksa988\\FreeKassa\\Facades\\": "src/Facades",
            "Maksa988\\FreeKassa\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Maksa988\\FreeKassa\\FreeKassaServiceProvider"
            ],
            "aliases": {
                "FreeKassa": "Maksa988\\FreeKassa\\Facades\\FreeKassa"
            }
        }
    }
}
